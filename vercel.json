{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 3008,
      "runtime": "python3.9"
    },
    "vercel_app.py": {
      "maxDuration": 60,
      "memory": 3008,
      "runtime": "python3.9"
    }
  },
  "regions": ["sin1"]
}

